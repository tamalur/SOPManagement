
@{
    TempData["PageTitle"] = "Publish SOP";
}

<div class="sopmessagebox">

    <div style="font-size:36px;font-weight:bold">

        @if (TempData["PageTitle"] != null)
        {
            @TempData["PageTitle"]
        }

        else if (Session["PageTitle"] != null)
        {

            @Session["PageTitle"]
        }


    </div>


    <font color="red"><b>ERROR:</b></font>SOP must be published to allow user access.


    <br />

    @if (ViewBag.SOPTitle != null)
    {
        <b> SOP Name:</b> @ViewBag.SOPTitle
    }

    else if (Session["SOPName"]!=null)
    {
        <b> SOP Name:</b> @Session["SOPName"].ToString()
    }

    <br />
    <br />
    @*<button onclick="goBack()">Go Back</button>*@
</div>




<script>
    function goBack() {
        window.history.back();
    }
</script>

